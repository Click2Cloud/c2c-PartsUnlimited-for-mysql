@model IEnumerable<PartsUnlimited.Models.Category>

@{
    var orderedList = Model.Where(x => x.Published == true).OrderBy(x => x.DisplayOrder);
}

@foreach (var category in orderedList)
{
    <li>@Html.ActionLink(category.Name, "Browse", "Store", new { CategoryId = category.CategoryId }, null)</li>
}
<li>@Html.ActionLink("More", "Index", "Store")</li>